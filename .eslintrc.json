{
    "plugins": ["node"],
    "extends": ["airbnb-base", "plugin:node/recommended"],
    "rules": {
      "no-underscore-dangle": 0,
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "minProperties": 6,
            "multiline": true,
            "consistent": true
          },
          "ObjectPattern": {
            "minProperties": 6,
            "multiline": true,
            "consistent": true
          }
        }
      ]
    }
}